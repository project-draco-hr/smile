{
  T neighbor=null;
  int index=-1;
  double dist=Double.MAX_VALUE;
  for (int i=0; i < data.length; i++) {
    if (q == data[i] && identicalExcluded) {
      continue;
    }
    double d=distance.d(q,data[i]);
    if (d < dist) {
      neighbor=data[i];
      index=i;
      dist=d;
    }
  }
  return new SimpleNeighbor<T>(neighbor,index,dist);
}
