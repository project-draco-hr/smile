{
  int n=0;
  int i=0;
  while (true) {
    if (i > j) {
      return n;
    }
    if (!isConsonant(i)) {
      break;
    }
    i++;
  }
  i++;
  while (true) {
    while (true) {
      if (i > j) {
        return n;
      }
      if (isConsonant(i)) {
        break;
      }
      i++;
    }
    i++;
    n++;
    while (true) {
      if (i > j) {
        return n;
      }
      if (!isConsonant(i)) {
        break;
      }
      i++;
    }
    i++;
  }
}
