{
  double[] lowerBound={0,Math.min(y)};
  double[] upperBound={y.length,Math.max(y)};
  extendBound(lowerBound,upperBound);
  double[][] data=new double[y.length][2];
  for (int i=0; i < data.length; i++) {
    data[i][0]=i;
    data[i][1]=y[i];
  }
  LinePlot plot=new LinePlot(data);
  plot.setID(id);
  plot.setColor(color);
  add(plot);
  return plot;
}
