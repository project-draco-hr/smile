{
  if (A.length != B[0].length) {
    throw new IllegalArgumentException(String.format("Matrix multiplication A' * B': %d x %d vs %d x %d",A.length,A[0].length,B.length,B[0].length));
  }
  int m=A[0].length;
  int n=B.length;
  int l=A.length;
  for (int i=0; i < m; i++) {
    for (int j=0; j < n; j++) {
      for (int k=0; k < l; k++) {
        C[i][j]+=A[k][i] * B[j][k];
      }
    }
  }
}
