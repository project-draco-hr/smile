{
  int n=x.length;
  int[] samples=new int[n];
  for (int i=0; i < n; i++) {
    samples[Math.randomInt(n)]++;
  }
  RegressionTree tree=new RegressionTree(attributes,x,y,maxNodes,nodeSize,mtry,order,samples,null);
  for (int i=0; i < n; i++) {
    if (samples[i] == 0) {
      double pred=tree.predict(x[i]);
synchronized (x[i]) {
        prediction[i]+=pred;
        oob[i]++;
      }
    }
  }
  return tree;
}
