{
  E e=heap[0];
  int i=0;
  while (lchild(i) < n) {
    int maxChild=rchild(i) < n && heap[lchild(i)].compareTo(heap[rchild(i)]) < 0 ? rchild(i) : lchild(i);
    if (e.compareTo(heap[maxChild]) < 0) {
      heap[i]=heap[maxChild];
      i=maxChild;
    }
 else {
      break;
    }
  }
  heap[i]=e;
}
