{
  if (shingles == null || shingles.isEmpty()) {
    return 0;
  }
  int[] bits=new int[BITS];
  for (  String s : shingles) {
    long hc=hf.hashString(s,Charsets.UTF_8).padToLong();
    for (int i=0; i < BITS; i++) {
      if (((hc >>> i) & 1) == 1) {
        bits[i]++;
      }
 else {
        bits[i]--;
      }
    }
  }
  long hash=0;
  long one=1;
  for (int i=0; i < BITS; i++) {
    if (bits[i] >= 0) {
      hash|=one;
    }
    one<<=1;
  }
  return hash;
}
