{
  int m=QR.length;
  int n=QR[0].length;
  double[][] R=new double[m][n];
  for (int i=0; i < n; i++) {
    for (int j=0; j < n; j++) {
      if (i < j) {
        R[i][j]=QR[i][j];
      }
 else       if (i == j) {
        R[i][j]=Rdiagonal[i];
      }
 else {
        R[i][j]=0.0;
      }
    }
  }
  return R;
}
